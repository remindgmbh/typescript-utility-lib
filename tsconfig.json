{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": ".",
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
